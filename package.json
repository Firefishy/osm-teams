{
  "name": "osm-teams",
  "description": "A teams API for OpenStreetMap",
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -w server.js server.js",
    "lint": "standard",
    "test": "NODE_ENV=test ava tests/*.test.js -c 1 --serial",
    "test-lint": "npm run test && npm run lint",
    "docs-api": "swagger-markdown -i api/api.yaml -o docs/api.md",
    "migrate": "knex migrate:latest --cwd db",
    "rollback": "knex migrate:rollback --cwd db",
    "build-docs": "npm run docs-api",
    "build": "npm run build-docs && next build",
    "start": "npm run build && node server.js"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.5.7",
    "fastify": "1.8.0",
    "fastify-boom": "^0.1.0",
    "fastify-cookie": "^2.0.2",
    "fastify-session": "^1.1.0",
    "fastify-swagger": "^0.15.1",
    "knex": "^0.15.2",
    "next": "^7.0.2",
    "nodemon": "^1.18.8",
    "passport-light": "^1.0.1",
    "passport-openstreetmap": "^0.1.2",
    "pg": "^7.5.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint-config-standard-react": "^7.0.2",
    "standard": "^12.0.1",
    "swagger-markdown": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developmentseed/osm-teams.git"
  },
  "keywords": [
    "osm"
  ],
  "license": "MIT",
  "author": "Development Seed <info@developmentseed.org>",
  "bugs": {
    "url": "https://github.com/developmentseed/osm-teams/issues"
  },
  "homepage": "https://github.com/developmentseed/osm-teams#readme"
}
